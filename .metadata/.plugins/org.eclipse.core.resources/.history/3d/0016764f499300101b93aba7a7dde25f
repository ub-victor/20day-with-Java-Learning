package System.out;

public class PositiveOrNegativeNumber2 {

	public static void main(String[] args) {
		
		int number = -13;
		
		if(number > 0) {
			System.out.println("The given number is positive");
		}else if (number < 0) {
			System.out.println("The given number is positive");
		}else {
			System.out.println("The given number is zero");
		}
		

	}

}
