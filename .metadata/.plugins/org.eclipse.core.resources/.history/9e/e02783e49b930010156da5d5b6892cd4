package day4;

public class LargestOf3Number {

	public static void main(String[] args) {
		int a =10, b=20, c=30; 
		
		if(a>b && a>c) {
			System.out.println("a is largest");
		}else if (b>a && b>c) {
			System.out.println("b is largest");
		}else {
			System.out.println("c is largest");
		}

	}

}
